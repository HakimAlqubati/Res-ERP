{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "arcanedev/log-viewer": "^11.0",
        "barryvdh/laravel-dompdf": "^3.0",
        "bezhansalleh/filament-language-switch": "^3.1",
        "bezhansalleh/filament-shield": "^3.2",
        "calebporzio/sushi": "^2.4",
        "carlos-meneses/laravel-mpdf": "^2.1",
        "coolsam/modules": "4",
        "filament/filament": "^3.2",
        "filament/forms": "^3.2",
        "filament/tables": "^3.2",
        "hugomyb/filament-media-action": "^3.1",
        "joshembling/image-optimizer": "^1.4",
        "laravel/framework": "^11.9",
        "laravel/passport": "^12.0",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.29",
        "league/flysystem-ftp": "^3.29",
        "maatwebsite/excel": "^3.1",
        "mokhosh/filament-rating": "^1.1",
        "nwidart/laravel-modules": "^11.0",
        "spatie/laravel-permission": "^6.9",
        "spatie/laravel-translatable": "^6.8",
        "ysfkaya/filament-phone-input": "^3.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php",
            "app/fifo_helpers.php",
            "app/auth_helpers.php",
            "app/attendance_helper.php",
            "app/payroll_helper.php",
            "app/Overrides/CustomChartWidget.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
