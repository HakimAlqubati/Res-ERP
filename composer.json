{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "arcanedev/log-viewer": "^11.0",
        "barryvdh/laravel-dompdf": "^3.0",
        "calebporzio/sushi": "^2.4",
        "carlos-meneses/laravel-mpdf": "^2.1",
        "filament/filament": "^4.0",
        "filament/forms": "^4.0",
        "filament/tables": "^4.0",
        "google/apiclient": "^2.18",
        "irazasyed/larasupport": "^1.1",
        "kreait/firebase-php": "^7.18",
        "laravel-notification-channels/fcm": "^5.1",
        "laravel/framework": "^11.9",
        "laravel/passport": "^12.0",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.29",
        "league/flysystem-ftp": "^3.29",
        "maatwebsite/excel": "^3.1",
        "nwidart/laravel-modules": "^11.0",
        "owen-it/laravel-auditing": "^14.0",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-backup": "^9.1",
        "spatie/laravel-medialibrary": "^11.12",
        "spatie/laravel-multitenancy": "4.0",
        "spatie/laravel-permission": "^6.9",
        "spatie/laravel-translatable": "^6.8"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "fakerphp/faker": "^1.23",
        "filament/upgrade": "4.0",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php",
            "app/auth_helpers.php"
        ],
          "exclude-from-classmap": [
        "node_modules/",
        "public/",
        "public/uploads/",
        "storage/",
        "bootstrap/cache/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {},
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": false

        },
        "exclude-from-classmap": ["node_modules/", "public/", "storage/", "bootstrap/cache/"]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
